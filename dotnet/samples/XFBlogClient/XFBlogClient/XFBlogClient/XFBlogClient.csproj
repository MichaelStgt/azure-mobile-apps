<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="fa-brands-400.ttf" />
    <None Remove="fa-regular-400.ttf" />
    <None Remove="fa-solid-900.ttf" />
    <None Remove="FiraSans-Regular.ttf" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="fa-brands-400.ttf" />
    <EmbeddedResource Include="fa-regular-400.ttf" />
    <EmbeddedResource Include="fa-solid-900.ttf" />
